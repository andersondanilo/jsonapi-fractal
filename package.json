  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.2.7",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andersondanilo/jsonapi-fractal.git"
  },
  "keywords": [
      "json api",
